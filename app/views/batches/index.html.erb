<div class="row">
  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        Batches
      </div>
      <div class="panel-body">

        <% @batches.each do |batch| %>
        <div class="row">
          <div class="col-md-7">
            </br>
            <%= link_to"Batch #{batch.number}", batch, class: "btn btn-primary" %>
            <p>Start date: <%= batch.start_date %></p>
            <p>End date: <%= batch.end_date %></p>
          </br>
          </div>
        </div>
        <% end %>

      </div>
    </div>

    <%= link_to "Add a batch", new_batch_path, class: "btn btn-primary" %>
  </div>
</div>
